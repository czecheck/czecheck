{
  "name": "czecheck",
  "version": "0.0.0-development",
  "description": "",
  "author": "Michal Vale≈° <michal.vales@xcd.cz>",
  "license": "CC0-1.0",
  "keywords": [
    "validate",
    "validation",
    "validator",
    "czech",
    "czechia",
    "nin"
  ],
  "homepage": "https://github.com/czecheck/czecheck",
  "bugs": {
    "url": "https://github.com/czecheck/czecheck/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/czecheck/czecheck.git"
  },
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "is-ci || husky install",
    "format": "prettier --config .prettierrc . --write",
    "format-check": "prettier --config .prettierrc . --check",
    "lint": "eslint .",
    "test": "jest",
    "clean": "rimraf dist",
    "prebuild": "node prebuild.cjs",
    "build": "unbuild",
    "postbuild": "rimraf src/index.ts",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "is-ci": "^3.0.1",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4",
    "unbuild": "^1.2.1",
    "husky": "^8.0.0"
  }
}
